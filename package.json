{
  "name": "broccoli-postcss-single",
  "version": "1.6.0",
  "description": "Postcss compiler for Broccoli, operates on individual files.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/broccoli-postcss-single"
  },
  "scripts": {
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm test"
  },
  "engines": {
    "node": ">= 6.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "broccoli-plugin",
    "css",
    "postcss",
    "postcss-runner"
  ],
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffjewiss/broccoli-postcss-single/issues"
  },
  "homepage": "https://github.com/jeffjewiss/broccoli-postcss-single",
  "dependencies": {
    "broccoli-caching-writer": "3.0.3",
    "include-path-searcher": "0.1.0",
    "mkdirp": "0.5.1",
    "object-assign": "4.1.1",
    "postcss": "6.0.22"
  },
  "devDependencies": {
    "async": "^2.0.0",
    "broccoli": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "^3.0.1",
    "glob": "^7.0.3",
    "husky": "1.0.0-rc.8",
    "mocha": "^5.0.0",
    "nyc": "^12.0.2",
    "postcss-color-rebeccapurple": "^3.0.0",
    "rimraf": "^2.4.0"
  }
}
